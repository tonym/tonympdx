{
  "name": "tonympdx",
  "version": "1.0.0",
  "description": "Tony's portfolio.",
  "scripts": {
    "analyze": "run-s build analyze:*",
    "analyze:js": "source-map-explorer dist/**/*.js dist/**/*.js.map",
    "build": "cross-env NODE_ENV=production run-s -l build:*",
    "build:clean": "rimraf dist",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:eleventy": "eleventy",
    "deploy": "npm run build && npm run deploy:only",
    "deploy:only": "gh-pages -d _site",
    "format": "prettier --write **/*.{js,s?css,yml} !dist",
    "start": "cross-env NODE_ENV=development run-p -l start:*",
    "start:webpack": "webpack --config webpack.dev.js ",
    "start:eleventy": "eleventy --serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@sanity/block-content-to-html": "^2.0.0",
    "@sanity/block-content-to-markdown": "^0.0.5",
    "@sanity/client": "^3.3.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "core-js": "^3.21.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "groq": "^2.29.3",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.5.2",
    "js-yaml": "^3.14.1",
    "markdown-it-anchor": "^8.6.4",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "source-map-explorer": "^2.5.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "css-loader": "^6.7.1"
  }
}
